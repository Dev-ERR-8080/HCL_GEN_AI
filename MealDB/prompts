    Check console for output
    <button onclick="fetchData('a')">Fetch Data</button>
</body>
<script >
    const baseurl = 'https://www.themealdb.com/api/json/v1/1/search.php?f=';    
    const min = 0;

     for (let char = 'a'; char <= 'z'; char = String.fromCharCode(char.charCodeAt(0) + 1)) {
        const wholeData = fetchData(char);
        wholeData.then(meals => {
            if (meals.length > min) {
                console.log(`Meals starting with '${char}':`, meals.length);
            }
        });
    }

    function fetchData(char) {
      return new Promise(async (resolve, reject) => {
        const response = await fetch(`${baseurl}${char}`); 
        if (!response.ok) throw new Error("Failed to fetch menu data");
        const result = await response.json();
        const meals = result.meals;
        console.log(`Meals starting with '${char}':`, meals.length);
        resolve(meals);
      }
      );
    };
</script>
</html>
